<h1>Create a Supervisor Account</h1>

<div class="form">
	<%= form_for Supervisor.new do |f| %>
	<%= f.label :fname %>
	<div class="box">
		<%=f.text_field :fname %>
	</div>
	<%=f.label :lname %>
	<div class="box">
		<%=f.text_field :lname%>
	<div class="box">
		<%=f.label :email%>
	<div class="box">
		<%=f.text_field :email%>
	</div>	
		<%=f.label :cellphone%>
	<div class="box">
		<%=f.text_field :cellphone%>
	</div>

	<%=f.label :address1%>
	<div class="box">
		<%=f.text_field :address1%>
	</div>

	<%=f.label :address2%>
	<div class="box">
		<%=f.text_field :address2%>
	</div>

	<%=f.label :city%>
	<div class="box">
		<%=f.text_field :city%>
	</div>

	<%=f.label :state%>
	<div class="box">
		<%=f.select :state, options_for_select(us_states) %>
	</div>

	<%=f.label :country%>
	<div class="box">
		<%=f.select :country, options_for_select(us_country) %>
	</div>

	<%=f.label :school%>
	<div class="box">
		<%=f.text_field :school%>
	</div>




	<%=f.submit :create%>

	<%end%>

