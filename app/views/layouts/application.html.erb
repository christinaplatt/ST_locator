	<%= csrf_meta_tags %>
</head>
<body>

	<nav class="navbar navbar-inverse navbar-fixed-top">
		<div class="container">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
					<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
				<!-- <a class="navbar-brand" href="#">EdNav -->
					<!-- <a id="home" title= "home" href="#">Home</a>
					<a id="map" title= "map" href="#">Map</a> -->
					<span class = "glyphicon glyphicon-home"></span>
					<span class = "glyphicon glyphicon-map-marker"></span>

				<!-- </a> -->
			</div>
			<div id="navbar" class="navbar-collapse collapse">
				<%= form_tag sessions_path, method: :post, class: "navbar-form navbar-right" do %>
					<div class="form-group">
						<input name="email" type="text" placeholder="Email" class="form-control" autocomplete="off" style="cursor: auto; background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;">
					</div>
					<div class="form-group">
						<input name="password" type="password" placeholder="Password" class="form-control" autocomplete="off" style="cursor: pointer; background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;">
					</div>
					<button type="submit" class="btn btn-success" href="#">Log in</button>
					<button type="submit" class="btn btn-primary" href="#">Sign up!</button>
					<!-- Add link to redirect to map or sign-up forms -->
				<% end %>
			</div><!--/.navbar-collapse -->
		</div>
	</nav>
	<!-- Message Area -->
	
	<div id="carousel" class="carousel slide" data-ride="carousel">
		<!-- Indicators -->
		<ol class="carousel-indicators">
			<li data-target="#carousel" data-slide-to="0" class="active"></li>
			<li data-target="#carousel" data-slide-to="1"></li>
			<li data-target="#carousel" data-slide-to="2"></li>
		</ol>
<nav class="navbar navbar-inverse navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          
        </div>
        <div id="navbar" class="navbar-collapse collapse">
           <%= form_tag sessions_path, method: :post, class: "navbar-form navbar-right" do %>
            <div class="form-group">
              <input name="email" type="text" placeholder="Email" class="form-control" autocomplete="off" style="cursor: auto; background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;">
            </div>
            <div class="form-group">
              <input name="password" type="password" placeholder="Password" class="form-control" autocomplete="off" style="cursor: pointer; background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;">
            </div>
            <button type="submit" class="btn btn-success">Sign in</button>
            <button type="submit" class="btn btn-primary"> <%= link_to "Sign up", '/new' %></button>
<!-- Add link to redirect to map or sign-up forms -->
         <% end %>
        </div><!--/.navbar-collapse -->
      </div>
    </nav>
<!-- Message Area -->
    <div class="jumbotron">
      <div class="container">
        <h1>Education Navigation</h1>
        <p>A travel app that decreases the distance between university supervisors and the student-teachers they mentor. </p>
        <p><a class="btn btn-primary btn-lg" href="#" role="button">Learn more »</a></p>
      </div>
    </div>



		<!-- Wrapper for slides -->
		<div class="carousel-inner">
			<div class="item active">
				<img class="first-slide" src="/assets/teacher1.jpg" alt="...">
				<div class="carousel-caption">
					<h3>Mentoring Teachers</h3>
				</div>
			</div>
			<div class="item">
				<img class="second-slide" src="/assets/student.jpg" alt="...">
				<div class="carousel-caption">
					<h3>Opening Communication</h3>
				</div>
			</div>
			<div class="item">
				<img class="third-slide" src="/assets/school2.jpg" alt="...">
				<div class="carousel-caption">
					<h3>Learning Efficiently</h3>
				</div>
			</div>
		</div>

		<!-- Controls -->
		<a class="left carousel-control" href="#carousel" role="button" data-slide="prev">
			<span class="glyphicon glyphicon-chevron-left"></span>
		</a>
		<a class="right carousel-control" href="#carousel" role="button" data-slide="next">
			<span class="glyphicon glyphicon-chevron-right"></span>
		</a>
	</div> <!-- Carousel -->

	<div class="container">
		<%= yield %>

		<hr>

		<footer>
			<p>© TKS 2015</p>
		</footer>
	</div> <!-- /container -->

	<div id="carousel-example-generic" class="carousel slide" data-ride="carousel" data-interval="3000">
